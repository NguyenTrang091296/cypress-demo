{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["cypress"]
  },
  "include": ["cypress/**/*.ts", "cypress/e2e/api/users/users-api.cy.js"],
  "exclude": ["node_modules"]
}
